{
  "private": true,
  "name": "wasm-postflop",
  "version": "0.2.2",
  "description": "Postflop solver powered by WebAssembly",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "build": "webpack",
    "serve": "node server.js",
    "lint": "eslint src/**/*.{ts,vue}",
    "format": "prettier --write src/**/*.{ts,vue}",
    "wasm": "npm run wasm:range && npm run wasm:tree && npm run wasm:solver-st && npm run wasm:solver-mt && npm run wasm:solver-patch && npm run wasm:solver-patch-cleanup",
    "wasm:range": "rustup run nightly wasm-pack build --out-dir ../../pkg/range rust/range",
    "wasm:tree": "rustup run nightly wasm-pack build --out-dir ../../pkg/tree rust/tree",
    "wasm:solver-st": "rustup run nightly wasm-pack build --target web --out-dir ../../pkg/solver-st rust/solver-st",
    "wasm:solver-mt": "rustup run nightly wasm-pack build --target web --out-dir ../../pkg/solver-mt rust/solver-mt",
    "wasm:solver-patch": "sed -i.bak \"s/cachedInt32Memory0 = new Int32Array(wasm.memory.buffer)/cachedInt32Memory0 = new Uint32Array(wasm.memory.buffer)/\" ./pkg/solver-mt/solver.js ./pkg/solver-st/solver.js",
    "wasm:solver-patch-cleanup": "rm ./pkg/solver-mt/solver.js.bak ./pkg/solver-st/solver.js.bak"
  },
  "dependencies": {
    "@heroicons/vue": "^2.0.13",
    "chart.js": "^4.2.0",
    "comlink": "^4.3.1",
    "detect-browser": "^5.3.0",
    "dexie": "^3.2.2",
    "pinia": "^2.0.29",
    "vue": "^3.2.45",
    "vue-chartjs": "^5.2.0",
    "vue-tippy": "^6.0.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@tailwindcss/forms": "^0.5.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "autoprefixer": "^10.4.13",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "cssnano": "^5.1.14",
    "eslint": "^8.32.0",
    "eslint-plugin-vue": "^9.9.0",
    "express": "^4.18.2",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.2",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.0.2",
    "prettier": "^2.8.3",
    "tailwindcss": "^3.2.4",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "url-loader": "^4.1.1",
    "vue-loader": "^17.0.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
